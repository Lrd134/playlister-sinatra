<% @artists.each do | artist | %>
  <h3>Name: <a href="/artists/<%=artist.slug%>"><%=artist.name%></a></h3>
  <% if artist.songs %>
      <% artist.songs.each_with_index do | song, index | %>
        <h4>Song #<%=index%>: <a href="/songs/<%=song.slug%>"><%=song.name%></a></h4>
      <% end %>
  <% end %>
<% end %>